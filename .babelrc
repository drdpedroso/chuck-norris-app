{
  "presets": [
    [
      "@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "transform-do-expressions",
    ["module-resolver", {
      "alias": {
        "components": "./src/components",
        "pages": "./src/pages",
        "index": "./src/index",
        "services": "./src/services"
      }
    }]
  ]
}
